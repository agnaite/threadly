<h3>Log in</h3>
<div class="container-fluid container-login ">
    <%= form_for(:session, url: '/login') do |f| %>
      <div class="form-group">
        <%= f.email_field :email, :placeholder => "Email", :id => "inputEmail", :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.password_field :password, :placeholder => "Password", :id => "inputPassword", :class => "form-control" %>
      </div>
      <%= f.submit "Log in", class: "btn-submit btn btn-default btn-block" %>
    <% end %>
</div>
